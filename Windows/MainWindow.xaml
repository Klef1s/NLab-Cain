<Window x:Class="NLab_Cain.Windows.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NLab_Cain.Windows"
        mc:Ignorable="d"
        AllowsTransparency="True" WindowStyle="None" WindowStartupLocation="CenterScreen" Background="Transparent"
        Height="768" Width="1280">
    <Grid>
        <Rectangle Style="{StaticResource Rectangle.MainBackground}"/>
        <StackPanel Orientation="Vertical" Margin="0,0,1180,0" Width="65" Height="668">
            <Button Style="{StaticResource Button.Home}" Margin="0,172,0,0"/>
            <Button Style="{StaticResource Button.Library}" Margin="0,48,0,0"/>
            <Button Style="{StaticResource Button.Settings}" Margin="0,48,0,0"/>
        </StackPanel>
        <Rectangle Fill="#CACACA" Margin="96,0,0,0"
                   Width="1" Height="668" HorizontalAlignment="Left" VerticalAlignment="Center"/>
    </Grid>
</Window>
